request.setAttribute("bootstrap_js",            LOCATOR.getFullPath("js/bootstrap.js")                              .substring(META_INF_RESOUCE_PATH_LENGTH) );
request.setAttribute("moment_js",               LOCATOR.getFullPath("moment.js")                                    .substring(META_INF_RESOUCE_PATH_LENGTH) );
%>
<script type="text/javascript" charset="utf8" src="<c:url value='/${jquery_js}'               />" ></script>
<script type="text/javascript" charset="utf8" src="<c:url value='/${bootstrap_js}'            />" ></script>
<script type="text/javascript" charset="utf8" src="<c:url value='/${moment_js}'               />" ></script>
